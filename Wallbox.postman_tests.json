					"name": "Add new charger",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 201\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"pm.test(\"New charger data is saved\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.name).to.eql(\"Pulsar Plus3\");",
									"    pm.expect(jsonData.type).to.eql(2);",
									"    pm.expect(jsonData.chargingPower).to.eql(7.4);",
									"    pm.expect(jsonData.color).to.eql(\"black\");",
									"    pm.expect(jsonData.installation).to.eql(\"none\");",
									"    pm.expect(jsonData.warranty).to.eql(\"2 years\");",
									"    pm.expect(jsonData.accessories).to.eql(true);",
									"});",
									""
